{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "node", 
    "lib": ["ES2015", "DOM"],
    "strict": true,
    "jsx": "react-jsx",
    "baseApi": "./src",
    "paths": {
      "@reduxjs/toolkit/query/react": ["./node_modules/@reduxjs/toolkit/query/react"]
    },
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true
  },
  "include": ["src"]
}


